    "fuse-box": "^1.3.122-preview.8",
    "lint-staged": "^3.3.1",
    "pre-commit": "^1.2.2",
    "prettier": "^0.19.0"
  },
  "dependencies": {
    "cxs": "^3.0.4",
    "inferno": "^1.3.0-rc.8",
    "inferno-component": "^1.3.0-rc.8",
    "inferno-router": "^1.3.0-rc.8"
  }
